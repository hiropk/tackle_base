<div id="<%= dom_id line %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= line.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= line.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Brand:</strong>
    <%= line.brand %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Pe rating:</strong>
    <%= line.pe_rating %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Length:</strong>
    <%= line.length %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Strand count:</strong>
    <%= line.strand_count %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Marker:</strong>
    <%= line.marker? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Price:</strong>
    <%= line.price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Purchase date:</strong>
    <%= line.purchase_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Notes:</strong>
    <%= line.notes %>
  </div>
</div>
